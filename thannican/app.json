{
  "expo": {
    "name": "Thannican",
    "slug": "Thannican",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "your-app-scheme",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#b7e3e1"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#b7e3e1"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "title": "Thannican"
    },
    "plugins": ["expo-router"]
  }
}
